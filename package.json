{
  "name": "sogeking-raydium-bot",
  "version": "2.0.0",
  "description": "Professional Raydium swap bot for Solana with monitoring and copy trading features",
  "main": "build/main.js",
  "scripts": {
    "start": "ts-node ./src/main.ts",
    "dev": "ts-node ./src/main.ts",
    "build": "tsc",
    "start:prod": "node ./build/main.js",
    "monitor": "ts-node ./src/main.ts --monitor",
    "snipe": "ts-node ./src/main.ts --snipe",
    "log-pools": "ts-node ./src/main.ts --log-pools",
    "copy": "ts-node ./src/main.ts --copy",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf build"
  },
  "keywords": [
    "solana",
    "raydium",
    "dex",
    "swap",
    "trading-bot",
    "crypto"
  ],
  "author": "DZ",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.4.3",
    "big.js": "^6.2.1",
    "bigint-buffer": "^1.1.5",
    "bn.js": "^5.2.1",
    "borsh": "^2.0.0",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "raydium-hakiun-sdk": "^1.3.7",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
